    "fastify": "^4.26.1",
    "fastify-cli": "^6.3.0",
    "fastify-plugin": "^4.0.0",
    "kafkajs": "^2.2.4",
    "object-hash": "^3.0.0",
    "web3": "^4.11.1",
    "websocket": "^1.0.35",
    "yaml": "^2.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
